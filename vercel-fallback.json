{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "buildCommand": "npm run build:packages && npm run build:serverless",
  "outputDirectory": "apps/web/.next",
  "installCommand": "npm install --include=dev --legacy-peer-deps",
  "framework": null,
  "env": {
    "TURBO_TELEMETRY_DISABLED": "1",
    "NEXT_TELEMETRY_DISABLED": "1",
    "NODE_ENV": "production",
    "FORCE_DYNAMIC": "1",
    "DISABLE_STATIC_GENERATION": "1",
    "VERCEL_SERVERLESS_ONLY": "1"
  },
  "build": {
    "env": {
      "TURBO_TELEMETRY_DISABLED": "1",
      "NEXT_TELEMETRY_DISABLED": "1",
      "NODE_ENV": "production",
      "SKIP_VALIDATION": "1",
      "FORCE_COLOR": "1",
      "FORCE_DYNAMIC": "1",
      "DISABLE_STATIC_GENERATION": "1",
      "NEXT_DISABLE_STATIC_GENERATION": "1",
      "SKIP_STATIC_GENERATION": "1",
      "VERCEL_SERVERLESS_ONLY": "1"
    }
  },
  "functions": {
    "apps/web/.next/server/pages/**/*.js": {
      "runtime": "nodejs20.x",
      "maxDuration": 30
    },
    "apps/web/.next/server/app/**/*.js": {
      "runtime": "nodejs20.x",
      "maxDuration": 30
    },
    "apps/web/src/app/**/*.tsx": {
      "runtime": "nodejs20.x",
      "maxDuration": 30
    },
    "apps/web/src/app/api/**/*.ts": {
      "runtime": "nodejs20.x",
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/apps/web/.next/server/app/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate"
        },
        {
          "key": "X-Deployment-Type",
          "value": "serverless-fallback"
        }
      ]
    }
  ]
}